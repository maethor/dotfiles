URxvt*modifier : mod1
URxvt*font : xft:monospace:pixelsize=11
!URxvt*font : xft:Bitstream Vera Sans Mono:pixelsize=10:style=Roman:antialias=false
!URxvt*shading : 20
!URxvt*tint : black
URxvt*scrollBar : false
!URxvt*perl-ext : tabbed
!URxvt.iso14755: false
!URxvt.keysym.M-b: perl:clipboard:paste
!URxvt.perl-ext-common: default,tabbed,clipboard
URxvt*perl-lib : /home/maethor/.urxvt/perl/ulhume
URxvt*perl-ext-common : focus

URxvt*urlLauncher : /usr/bin/chromium

URxvt*urgentOnBell: true

