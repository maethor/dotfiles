urxvt*modifier : mod1
urxvt*font : xft:monospace:pixelsize=11
!urxvt*font : xft:Bitstream Vera Sans Mono:pixelsize=11:style=Roman:antialias=true
!urxvt*shading : 20
!urxvt*tint : black
urxvt*scrollBar : false
!urxvt*perl-ext : tabbed
!URxvt.iso14755: false
!URxvt.keysym.M-b: perl:clipboard:paste
!URxvt.perl-ext-common: default,tabbed,clipboard

urxvt*urlLauncher : /usr/bin/chromium

!  - color scheme
